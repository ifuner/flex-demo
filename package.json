{
  "name": "flex-demo",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@baoji/vue-boot": "^0.0.1",
    "vue-highlightjs": "^1.3.3"
  },
  "devDependencies": {
    "@baoji/fie-toolkit-vue": "^0.5.8",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1"
  },
  "scripts": {},
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "iOS >= 7",
          "Android >= 4"
        ]
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "plugins": [
      "html",
      "standard",
      "promise"
    ],
    "extends": "standard",
    "rules": {
      "space-before-function-paren": 0,
      "arrow-parens": 0,
      "no-new": 0,
      "prefer-promise-reject-errors": 0,
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "double",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  }
}
